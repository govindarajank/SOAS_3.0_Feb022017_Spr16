            <xml-property name="queryText"><![CDATA[SELECT 
N.FIRST_NM,
N.LAST_NM,
N.ENTITY_ID,
R.OLE_PTRN_BARCD,
R.PTRN_Q_POS,
R.ITM_ID,
B.TITLE,
B.AUTHOR,
I.CALL_NUMBER,
I.CALL_NUMBER_PREFIX,
ST.ITEM_AVAIL_STAT_CD,
R.HOLD_EXP_DATE,
I.DUE_DATE_TIME,
R.CRTE_DT_TIME,
(SELECT E.EMAIL_ADDR FROM KRIM_ENTITY_EMAIL_T E WHERE I.CURRENT_BORROWER = E.ENTITY_ID)
FROM
OLE_DLVR_RQST_T R 
JOIN KRIM_ENTITY_NM_T N ON R.OLE_PTRN_ID = N.ENTITY_ID
JOIN OLE_DS_ITEM_T I ON R.ITM_ID = I.BARCODE
JOIN OLE_DS_HOLDINGS_T H ON I.HOLDINGS_ID = H.HOLDINGS_ID
JOIN OLE_DS_BIB_INFO_T B ON H.BIB_ID = B.BIB_ID
JOIN OLE_DLVR_ITEM_AVAIL_STAT_T ST ON ST.ITEM_AVAIL_STAT_ID=I.ITEM_STATUS_ID]]></xml-property>
