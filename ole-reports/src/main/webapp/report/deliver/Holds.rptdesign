            <method name="beforeOpen"><![CDATA[this.queryText = this.queryText + " WHERE R.PTRN_Q_POS IN ('"+params["ptrnQuePos"].value.join("','")+"')";
this.queryText=this.queryText+" AND ST.ITEM_AVAIL_STAT_ID IN ('"+params["itemStatus"].value.join("','")+"')";

if(params["itemStatus"].length == 1){
	if(params["itemStatus"].value[0] == '22'){
		if(params["holdExpFromDate"].value !=null &amp;&amp; params["holdExpToDate"].value==null)
			this.queryText=this.queryText+" AND R.HOLD_EXP_DATE >='"+params["holdExpFromDate"].value+"'";
		else if(params["holdExpFromDate"].value ==null &amp;&amp; params["holdExpToDate"].value!=null)
			this.queryText=this.queryText+" AND R.HOLD_EXP_DATE<='"+params["holdExpToDate"].value+"'";
		else if(params["holdExpFromDate"].value !=null &amp;&amp; params["holdExpToDate"].value!=null)
			this.queryText=this.queryText+" AND R.HOLD_EXP_DATE BETWEEN '"+params["holdExpFromDate"].value+"' AND '"+params["holdExpToDate"].value+"'";
		else
			this.queryText=this.queryText+"";
	}
}

this.queryText=this.queryText+ " ORDER BY R.HOLD_EXP_DATE DESC, R.ITM_ID ASC,R.PTRN_Q_POS ASC";
]]></method>
